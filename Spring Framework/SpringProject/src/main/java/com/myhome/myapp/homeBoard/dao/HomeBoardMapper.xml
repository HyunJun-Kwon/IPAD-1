<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.myhome.myapp.homeBoard.dao.IHomeBoardRepository">
	<resultMap id="getList"
		type="com.myhome.myapp.homeBoard.model.HomeBoardVO">
		<result property="admCd" column="ADM_CD"/>
		<result property="regionName" column="REGION_NAME"/>
		<result property="regionNameDetail" column="REGION_NAME_DETAIL"/>
	</resultMap>
	
	<select id="list" resultMap="getList">
		select * from region
	</select>
</mapper>